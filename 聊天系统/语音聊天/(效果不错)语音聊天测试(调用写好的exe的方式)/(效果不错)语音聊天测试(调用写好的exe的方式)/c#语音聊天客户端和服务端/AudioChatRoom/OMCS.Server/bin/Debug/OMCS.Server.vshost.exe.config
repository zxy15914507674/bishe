<?xml version="1.0"?>
<configuration>
  <appSettings>   
    <!--服务器监听的端口-->
    <add key="Port" value="9800"/>   
    <!--是否记录安全日志文件 Omcs.txt-->
    <add key="SecurityLogEnabled" value="true"/>    
    <!--UserID的最大长度-->
    <add key="MaxLengthOfUserID" value="11"/>
    
    <!--摄像头采集视频的帧率-->
    <add key="CameraFramerate" value="20"/>   
    <!--音频编码质量,Low/Middle/High。-->
    <add key="AudioQuality" value="High"/>       
    <!--桌面帧率-->
    <add key="DesktopFramerate" value="3"/>         
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
